{
  "extends": "../tsconfig.json",
  "files": ["../cypress.config.ts"],
  "compilerOptions": {
    "baseUrl": "../src",
    "outDir": "e2e/",
    "module": "commonjs",
    "removeComments": true,
    "strict": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "target": "es5",
    "lib": ["es2015", "es2017", "dom"],
    "types": ["cypress", "node"]
  },
  "include": ["**/*.ts", "**/*.cy.js", "support/*.ts"],
  "exclude": ["node_modules"]
}
